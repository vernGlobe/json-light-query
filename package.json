{
  "name": "@vernglobe/json-light-query",
  "version": "2.0.0",
  "description": "query to jsonobject/array and return summary of data such as total value of specified attributes.",
  "main": "dist/main.js",
  "scripts": {
    "start": "tsc && node dist/main.js",
    "lint": "eslint . --ext .ts",
    "test:jest": "jest --coverage",
    "test": "exit 0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "src/__tests__/mock-payload/",
      "dist/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vernGlobe/json-light-query.git"
  },
  "keywords": [
    "query",
    "json-light-query",
    "light-query",
    "vernglobe",
    "vern"
  ],
  "author": "vernglobe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vernGlobe/json-light-query/issues"
  },
  "homepage": "https://github.com/vernGlobe/json-light-query#readme",
  "publishConfig": {
    "@vernglobe:registry":"https://npm.pkg.github.com/"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "jest": "^28.1.1",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
